    "test:single": "jest -t router.test.ts",
    "lint": "eslint ./static/js/index.js",
    "webpack:serve": "webpack serve --mode=development --progress",
    "webpack:dev": "cross-env NODE_ENV=development npx webpack --mode=development --config webpack.config.dev.js",
    "webpack:prod": "cross-env NODE_ENV=production npx webpack --mode=production --config webpack.config.prod.js",
    "build-tmpl": "npm run build-tmpl:tsc && npm run build-tmpl:putJS && npm run build-tmpl:importComponentsJS",
    "build-tmpl:tsc": "rm -rf static/build && tsc",
    "build-tmpl:putJS": "node helper/putJS.js",
    "build-tmpl:importComponentsJS": "node helper/importComponentsJS.js",
    "build-CSS": "npm run build-CSS:componentsCSS && npm run build-CSS:importCSSInIndex",
    "build-CSS:componentsCSS": "node helper/importComponentsCSS.js",
    "build-CSS:importCSSInIndex": "node helper/importCSSInIndex.js",
    "build:postCSS": "rm -rf static/css/components && postcss ts/**/*.css --dir static/css/components",
    "build": "npm run build-tmpl && npm run build-CSS",
    "helper:makeCSS": "node helper/makeCSS.js",
    "helper:makeDir": "node helper/makeDir.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test"
